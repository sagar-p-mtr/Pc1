#include <stdio.h>
#include <omp.h>
int is_prime(int num) {
    if (num <= 1) return 0;
    if (num == 2) return 1;
    if (num % 2 == 0) return 0;
    for (int i = 3; i * i <= num; i += 2)
        if (num % i == 0) return 0;
    return 1;
}
int main() {
    int n, i;
    double start, end;
    printf("Enter the value of n: ");
    scanf("%d", &n);
    printf("\nSerial Execution:\n");
    start = omp_get_wtime();
    for (i = 1; i <= n; i++)
        if (is_prime(i)) printf("%d ", i);
    end = omp_get_wtime();
    printf("\nTime taken (Serial): %f seconds\n", end - start);
    printf("\nParallel Execution (Note: order may vary due to thread scheduling):\n");
    start = omp_get_wtime();
    #pragma omp parallel for shared(n) schedule(dynamic)
    for (i = 1; i <= n; i++) {
        if (is_prime(i)) {
            #pragma omp critical
            printf("%d ", i);
        }
    }
    end = omp_get_wtime();
    printf("\nTime taken (Parallel): %f seconds\n", end - start);
    return 0;
}

gcc -fopenmp p4.c -o p4

./p4
